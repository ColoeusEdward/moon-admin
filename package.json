{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "eslint": "eslint --ext .ts,.tsx,.vue src --fix",
    "format": "prettier --write src/**/*.{vue,ts,tsx}",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "serve": "vite preview",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": []
  },
  "dependencies": {
    "axios": "^0.21.1",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.15.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-beta.5",
    "vfonts": "^0.1.0",
    "vue": "^3.1.5",
    "vue-grid-layout": "3.0.0-beta1",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vicons/antd": "^0.10.0",
    "@vicons/carbon": "^0.10.0",
    "@vicons/fa": "^0.10.0",
    "@vicons/fluent": "^0.10.0",
    "@vicons/ionicons4": "^0.10.0",
    "@vicons/ionicons5": "^0.10.0",
    "@vicons/material": "^0.10.0",
    "@vicons/tabler": "^0.10.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.1.5",
    "css-render": "^0.15.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  }
