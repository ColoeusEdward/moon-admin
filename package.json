{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "eslint": "eslint --ext .ts,.tsx,.vue src --fix",
    "format": "prettier --write src/**/*.{vue,ts,tsx}",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
    "builds": "vue-tsc --noEmit",
    "serve": "vite preview",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pc": "lint-staged",
    "upup": "yarn upgrade-interactive"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": []
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bent": "^7.3.12",
    "c-scrollbar": "^1.0.1",
    "echarts": "^5.2.1",
    "mockjs": "^1.1.0",
    "naive-ui": "2.19.9",
    "nprogress": "^0.2.0",
    "pinia": "2.0.0-rc.13",
    "rxjs": "^7.3.0",
    "socket.io-client": "^4.2.0",
    "uuid": "^8.3.2",
    "vfonts": "^0.1.0",
    "vue": "3.2.13",
    "vue-echarts": "^6.0.0",
    "vue-router": "^4.0.11",
    "vue3-grid-layout": "^1.0.0"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vicons/antd": "^0.11.0",
    "@vicons/carbon": "^0.11.0",
    "@vicons/fa": "^0.11.0",
    "@vicons/fluent": "^0.11.0",
    "@vicons/ionicons4": "^0.11.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vicons/material": "^0.11.0",
    "@vicons/tabler": "^0.11.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "3.2.13",
    "css-render": "^0.15.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.43.2",
    "sass-loader": "^12.1.0",
    "typescript": "^4.4.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "2.5.10",
    "vue-tsc": "^0.3.0"
  }
}
