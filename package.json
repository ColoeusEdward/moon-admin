{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "eslint": "eslint --ext .ts,.tsx,.vue src --fix",
    "build": "vue-tsc --noEmit && vite build --mode production",
    "serve": "vite preview",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,vue}": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.15.4",
    "vfonts": "^0.1.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vicons/antd": "^0.10.0",
    "@vicons/carbon": "^0.10.0",
    "@vicons/fa": "^0.10.0",
    "@vicons/fluent": "^0.10.0",
    "@vicons/ionicons4": "^0.10.0",
    "@vicons/ionicons5": "^0.10.0",
    "@vicons/material": "^0.10.0",
    "@vicons/tabler": "^0.10.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.1.4",
    "css-render": "^0.15.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "vue-tsc": "^0.2.0"
  }
}
