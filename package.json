{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "eslint": "eslint --ext .ts,.tsx,.vue src --fix",
    "format": "prettier --write src/**/*.{vue,ts,tsx}",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
    "builds": "vue-tsc --noEmit",
    "buildf": "vite build --mode production",
    "serve": "vite preview",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pc": "lint-staged",
    "upup": "yarn upgrade-interactive"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": []
  },
  "dependencies": {
    "@icon-park/vue-next": "^1.4.0",
    "@vueuse/core": "^8.5.0",
    "axios": "^0.21.1",
    "bent": "^7.3.12",
    "echarts": "^5.2.1",
    "htmlparser2": "^8.0.1",
    "mockjs": "^1.1.0",
    "naive-ui": "2.29.0",
    "nprogress": "^0.2.0",
    "pinia": "2.0.9",
    "ramda": "^0.28.0",
    "rxjs": "^7.5.5",
    "socket.io-client": "^4.2.0",
    "uuid": "^8.3.2",
    "vfonts": "^0.1.0",
    "vue": "3.2.36",
    "vue-echarts": "^6.0.0",
    "vue-jsonp": "^2.0.0",
    "vue-router": "^4.0.15",
    "vue3-grid-layout": "^1.0.0"
  },
  "devDependencies": {
    "@css-render/plugin-bem": "^0.15.8",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^16.3.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-vue": "2.3.3",
    "@vitejs/plugin-vue-jsx": "1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "autoprefixer": "^10.4.2",
    "css-render": "^0.15.8",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2",
    "sass": "^1.43.2",
    "sass-loader": "^12.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "2.9.9",
    "vue-tsc": "^0.34.16"
  },
  "packageManager": "yarn@3.1.1"
}
